.theme_container
  margin 0 0 60px 0
  .top_anchor
    text-align center
    position relative
    z-index 90
    overflow hidden
    .front_back
      float left
    .ours_yours
      float right
      display none
    .toggle
      margin 15px
      overflow hidden
  .themes
    box_shadow(0,color_two,10px,inset)
    background color_four
    position relative
    z-index 250
    height 500px
    overflow auto
    .thumb
      margin 30px 0 0 15px
      cursor pointer
      float left
      height 120px
      width 220px
      padding 60px 0 0 15px
      position relative
      .bg,.fg,.bg_bg,.fg_bg
        transition()
        position absolute
      .bg
        z-index 200
        transform_matrix(.25,-0.33,0,1,100,-25)
      .fg
        z-index 201
        transform_matrix(.9,0.05,-0.01,1,0,0)
      .fg_bg,.bg_bg
        height 90px
        width 158px
        z-index 199
        background color_two
      .fg_bg
        transform_matrix(.9,0.05,0.09,.2,-5,35)
      .bg_bg
        transform_matrix(.25,-0.33,.1,.1,95,15)
    .active
      img
        border-color transparent
      border 4px solid color_seven
      border_radius(15px)
      margin 26px -4px -4px 11px
      background color_three
    .hover
      opacity(1)
  .themes.back
    .thumb
      .bg
        transform_matrix(.9,0.05,-0.01,1,0,0)
      .fg
        transform_matrix(.25,-0.33,0,1,100,-25)
      .fg_bg
        transform_matrix(.25,-0.33,.1,.1,95,15)
      .bg_bg
        transform_matrix(.9,0.05,0.09,.2,-5,35)

